    <application
        android:allowBackup="true"
        android:icon="@drawable/wolf"
        android:label="WolfTravels"
        android:roundIcon="@drawable/wolf"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".KonarkActivity"/>
        <activity android:name=".PuriActivity" />
        <activity android:name=".JagarActivity" />
        <activity android:name=".AshokaActivity" />
        <activity android:name=".ChilkaActivity" />
        <activity android:name=".WaterActivity" />
        <activity android:name=".NandanActivity"></activity>
    </application>

</manifest>